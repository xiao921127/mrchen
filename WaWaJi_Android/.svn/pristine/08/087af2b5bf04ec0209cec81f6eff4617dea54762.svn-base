<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layout"
    android:background="@drawable/ic_actionbar_bg">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/item_title" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="524px"
            android:background="@drawable/white_datails"
            android:layout_marginRight="15px"
            android:layout_marginLeft="15px"
            android:layout_marginTop="100px"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="120px">
                <ImageView
                    android:id="@+id/play_img"
                    android:layout_width="60px"
                    android:layout_height="80px"
                    android:layout_marginLeft="20px"
                    android:layout_marginTop="20px"/>
                <TextView
                    android:id="@+id/name_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="28px"
                    android:textColor="@color/black_text"
                    android:layout_marginTop="30px"
                    android:layout_marginLeft="100px"/>
                <TextView
                    android:id="@+id/time_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="24px"
                    android:textColor="@color/gray_text"
                    android:layout_marginTop="65px"
                    android:layout_marginLeft="100px"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/success"
                    android:textColor="@color/empty"
                    android:textSize="28px"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20px"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/gray_bg"
                android:layout_marginRight="20px"
                android:layout_marginLeft="20px"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/number"
                    android:textSize="28px"
                    android:textColor="@color/gray_text"/>
                <TextView
                    android:id="@+id/number_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:textSize="28px"
                    android:textColor="@color/black_text"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/gray_bg"
                android:layout_marginRight="20px"
                android:layout_marginLeft="20px"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/serial_number"
                    android:textSize="28px"
                    android:textColor="@color/gray_text"/>
                <TextView
                    android:id="@+id/serial_number_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:textSize="28px"
                    android:textColor="@color/black_text"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/gray_bg"
                android:layout_marginRight="20px"
                android:layout_marginLeft="20px"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/current_state"
                    android:textSize="28px"
                    android:textColor="@color/gray_text"/>
                <TextView
                    android:id="@+id/current_state_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="28px"
                    android:textColor="@color/black_text"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/gray_bg"
                android:layout_marginRight="20px"
                android:layout_marginLeft="20px"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/exchange_play_currency"
                    android:textSize="28px"
                    android:textColor="@color/gray_text"/>
                <TextView
                    android:id="@+id/exchange_currency_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:textSize="28px"
                    android:textColor="@color/black_text"/>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/rl_one"
            android:layout_width="match_parent"
            android:layout_height="110px"
            android:layout_marginTop="660px"
            android:layout_marginRight="15px"
            android:layout_marginLeft="15px"
            android:background="@drawable/white_datails">
            <TextView
                android:layout_width="480px"
                android:layout_height="wrap_content"
                android:text="@string/exchange_explain"
                android:textSize="24px"
                android:gravity="center_vertical"
                android:textColor="@color/gray_text"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20px"/>
            <ImageView
                android:layout_width="130px"
                android:layout_height="55px"
                android:src="@drawable/currenc_exchange"
                android:layout_alignParentRight="true"
                android:layout_marginRight="30px"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_two"
            android:layout_width="match_parent"
            android:layout_height="110px"
            android:layout_marginTop="800px"
            android:layout_marginLeft="15px"
            android:layout_marginRight="15px"
            android:background="@drawable/white_datails">
            <TextView
                android:layout_width="480px"
                android:layout_height="wrap_content"
                android:text="@string/mail_explain"
                android:textSize="24px"
                android:gravity="center_vertical"
                android:textColor="@color/gray_text"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20px"/>
            <ImageView
                android:layout_width="130px"
                android:layout_height="55px"
                android:layout_alignParentRight="true"
                android:layout_marginRight="30px"
                android:src="@drawable/deliver"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_three"
            android:layout_width="match_parent"
            android:layout_height="320px"
            android:layout_marginTop="660px"
            android:layout_marginRight="15px"
            android:layout_marginLeft="15px"
            android:background="@drawable/white_datails">
            <TextView
                android:id="@+id/name_user_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="28px"
                android:textColor="@color/black_text"
                android:layout_marginLeft="20px"
                android:layout_marginTop="30px"/>
            <TextView
                android:id="@+id/address_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/gray_text"
                android:textSize="26px"
                android:layout_marginLeft="20px"
                android:layout_marginTop="65px"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/gray_bg"
                android:layout_marginRight="20px"
                android:layout_marginLeft="20px"
                android:layout_marginTop="119px"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px"
                android:layout_marginTop="120px">
                <TextView
                    android:id="@+id/order_user"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="28px"
                    android:textColor="@color/gray_text"/>
                <TextView
                    android:id="@+id/state_user"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="28px"
                    android:textColor="@color/black_text"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/gray_bg"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/user_rl"
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:layout_marginLeft="20px"
                android:layout_marginRight="20px"
                android:layout_marginTop="220px">
                <TextView
                    android:id="@+id/name_user"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textSize="28px"
                    android:textColor="@color/gray_text"/>
                <TextView
                    android:id="@+id/number_user"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="28px"
                    android:textColor="@color/black_text"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="40px"/>
                <ImageView
                    android:layout_width="30px"
                    android:layout_height="50px"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_msg_arrow"/>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
